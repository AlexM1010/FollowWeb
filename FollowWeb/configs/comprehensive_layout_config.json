{
  "input_file": "examples/followers_following.json",
  "output_file_prefix": "Output/FollowWeb",
  "data_source": {
    "source": "instagram",
    "freesound": {
      "api_key": null,
      "query": null,
      "tags": null,
      "max_samples": 1000,
      "include_similar": true
    }
  },
  "renderer": {
    "renderer_type": "pyvis",
    "sigma_interactive": {
      "height": "100vh",
      "width": "100%",
      "enable_webgl": true,
      "enable_audio_player": true
    }
  },
  "pipeline": {
    "strategy": "k-core",
    "ego_username": null,
    "enable_strategy": true,
    "enable_analysis": true,
    "enable_visualization": true,
    "enable_community_detection": true,
    "enable_centrality_analysis": true,
    "enable_path_analysis": true
  },
  "analysis_mode": {
    "mode": "full",
    "sampling_threshold": 5000,
    "max_layout_iterations": null,
    "enable_fast_algorithms": false,
    "skip_path_analysis": false
  },

  "fame_analysis": {
    "find_paths_to_all_famous": true,
    "contact_path_target": null,
    "min_followers_in_network": 5,
    "min_fame_ratio": 5.0
  },
  "k_values": {
    "strategy_k_values": {
      "k-core": 10,
      "reciprocal_k-core": 10,
      "ego_alter_k-core": 10
    },
    "default_k_value": 10,
    "allow_cli_override": true
  },
  "output": {
    "custom_output_directory": null,
    "generate_html": true,
    "generate_png": true,
    "generate_reports": true,
    "enable_timing_logs": true,
    "formatting": {
      "indent_size": 2,
      "group_related_settings": true,
      "highlight_key_values": true,
      "use_human_readable_labels": true,
      "emoji": {
        "fallback_level": "full"
      }
    }
  },
  "visualization": {
    "node_size_metric": "degree",
    "base_node_size": 8.0,
    "node_size_multiplier": 4.0,
    "scaling_algorithm": "logarithmic",
    "edge_thickness_metric": "weight",
    "base_edge_thickness": 1.0,
    "base_edge_width": 0.4,
    "edge_width_multiplier": 2.0,
    "edge_width_scaling": "logarithmic",
    "intra_community_color": "#c0c0c0",
    "bridge_color": "#6e6e6e",
    "show_labels": true,
    "show_tooltips": true,
    "show_legend": true,
    "font_size": 8,
    "node_alpha": 0.9,
    "edge_alpha": 0.4,
    "pyvis_interactive": {
      "width": "100%",
      "height": "90vh",
      "bgcolor": "#ffffff",
      "font_color": "#000000",
      "physics_solver": "forceAtlas2Based"
    },
    "static_image": {
      "width": 2400,
      "height": 2400,
      "dpi": 300,
      "edge_arrow_size": 8
    },
    "layout": {
      "algorithm": "spring",
      "align_with_html": false,
      "spring": {
        "k": 0.8,
        "iterations": 300,
        "spring_length": 1.2,
        "spring_constant": 1.5,
        "repulsion_strength": 1.2,
        "attraction_strength": 0.8,
        "center_gravity": 0.02,
        "damping": 0.85,
        "min_velocity": 0.005,
        "max_displacement": 15.0,
        "enable_multistage": true
      }
    }
  },
  "checkpoint": {
    "checkpoint_dir": "checkpoints",
    "checkpoint_interval": 50,
    "max_runtime_hours": null,
    "verify_existing_sounds": false
  }
}