{
  "input_file": "examples/followers_following.json",
  "output_file_prefix": "Output/FollowWeb",
  "pipeline": {
    "strategy": "k-core",
    "skip_analysis": false,
    "ego_username": null
  },
  "analysis": {
    "contact_path_target": null
  },
  "fame_analysis": {
    "find_paths_to_all_famous": false,
    "min_followers_in_network": 5,
    "min_fame_ratio": 5.0
  },
  "pruning": {
    "k_values": {
      "k-core": 1,
      "reciprocal_k-core": 1,
      "ego_alter_k-core": 1
    },
    "default_k_value": 1
  },
  "visualization": {
    "node_size_metric": "degree",
    "base_node_size": 4.0,
    "node_size_multiplier": 3.0,
    "scaling_algorithm": "logarithmic",
    "base_edge_width": 0.3,
    "edge_width_multiplier": 1.5,
    "edge_width_scaling": "logarithmic",
    "intra_community_color": "#c0c0c0",
    "bridge_color": "#6e6e6e",
    "pyvis_interactive": {
      "width": "100%",
      "height": "90vh",
      "notebook": false,
      "show_labels": true,
      "show_tooltips": true,
      "physics_solver": "forceAtlas2Based"
    },
    "static_image": {
      "generate": true,
      "layout": "spring",
      "with_labels": false,
      "font_size": 6,
      "image_size_inches": [30, 30],
      "dpi": 300,
      "edge_alpha": 0.4,
      "node_alpha": 0.9,
      "edge_arrow_size": 6,
      "show_legend": true
    },
    "png_layout": {
      "force_spring_layout": true,
      "align_with_html": false,
      
      "spring": {
        "k": 0.8,
        "iterations": 300,
        "spring_length": 1.2,
        "spring_constant": 1.5,
        "repulsion_strength": 1.2,
        "attraction_strength": 0.8,
        "center_gravity": 0.02,
        "gravity_x": 0.0,
        "gravity_y": 0.0,
        "damping": 0.85,
        "min_velocity": 0.005,
        "max_displacement": 15.0,
        "enable_multistage": true,
        "initial_k_multiplier": 2.0,
        "final_k_multiplier": 0.7
      },
      
      "kamada_kawai": {
        "max_iterations": 1500,
        "tolerance": 1e-7,
        "distance_scale": 1.2,
        "spring_strength": 1.5,
        "pos_tolerance": 1e-5,
        "weight_function": "path"
      },
      
      "circular": {
        "radius": null,
        "center": null,
        "start_angle": 0.0,
        "angular_spacing": "degree",
        "group_by_community": true,
        "community_separation": 0.3
      },
      
      "shell": {
        "shell_spacing": 1.5,
        "center_shell_radius": 0.3,
        "arrange_by_community": true,
        "arrange_by_centrality": false,
        "centrality_metric": "degree",
        "max_shells": 8,
        "nodes_per_shell": null
      }
    },
    "performance": {
      "fast_mode": false,
      "skip_path_analysis": false,
      "max_layout_iterations": null,
      "use_sampling_threshold": 5000
    }
  },
  "output": {
    "enable_time_logging": true,
    "custom_output_directory": null,
    "create_directories": true
  }
}