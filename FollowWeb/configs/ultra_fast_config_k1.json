{
  "input_file": "examples/followers_following.json",
  "output_file_prefix": "Output/FollowWeb",
  "pipeline": {
    "strategy": "k-core",
    "skip_analysis": false,
    "ego_username": null
  },
  "analysis": {
    "contact_path_target": null
  },
  "fame_analysis": {
    "find_paths_to_all_famous": false,
    "min_followers_in_network": 10,
    "min_fame_ratio": 10.0
  },
  "pruning": {
    "k_values": {
      "k-core": 1,
      "reciprocal_k-core": 1,
      "ego_alter_k-core": 1
    },
    "default_k_value": 1
  },
  "visualization": {
    "node_size_metric": "degree",
    "base_node_size": 6.0,
    "node_size_multiplier": 5.0,
    "scaling_algorithm": "logarithmic",
    "base_edge_width": 0.5,
    "edge_width_multiplier": 2.0,
    "edge_width_scaling": "logarithmic",
    "intra_community_color": "#c0c0c0",
    "bridge_color": "#6e6e6e",
    "pyvis_interactive": {
      "width": "100%",
      "height": "90vh",
      "notebook": false,
      "show_labels": true,
      "show_tooltips": true,
      "physics_solver": "forceAtlas2Based"
    },
    "static_image": {
      "generate": false,
      "layout": "circular",
      "with_labels": false,
      "font_size": 8,
      "image_size_inches": [15, 15],
      "dpi": 150,
      "spring_k": 0.3,
      "spring_iterations": 10,
      "edge_alpha": 0.3,
      "node_alpha": 0.8,
      "edge_arrow_size": 8,
      "show_legend": true
    },
    "performance": {
      "fast_mode": true,
      "skip_path_analysis": true,
      "max_layout_iterations": 1,
      "use_sampling_threshold": 1000
    }
  },
  "output": {
    "enable_time_logging": true,
    "custom_output_directory": null,
    "create_directories": true
  }
}