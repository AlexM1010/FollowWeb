{
  "input_file": "examples/followers_following.json",
  "output_file_prefix": "Output/FollowWeb",
  "pipeline": {
    "strategy": "k-core",
    "ego_username": null,
    "enable_strategy": true,
    "enable_analysis": true,
    "enable_visualization": true,
    "enable_community_detection": true,
    "enable_centrality_analysis": true,
    "enable_path_analysis": true
  },
  "analysis_mode": {
    "mode": "fast",
    "sampling_threshold": 3000,
    "max_layout_iterations": 50,
    "enable_fast_algorithms": true,
    "skip_path_analysis": false
  },
  "fame_analysis": {
    "find_paths_to_all_famous": false,
    "contact_path_target": null,
    "min_followers_in_network": 3,
    "min_fame_ratio": 3.0
  },
  "k_values": {
    "strategy_k_values": {
      "k-core": 1,
      "reciprocal_k-core": 1,
      "ego_alter_k-core": 1
    },
    "default_k_value": 1,
    "allow_cli_override": true
  },
  "output": {
    "custom_output_directory": null,
    "generate_html": true,
    "generate_png": true,
    "generate_reports": true,
    "enable_timing_logs": true,
    "formatting": {
      "indent_size": 2,
      "group_related_settings": true,
      "highlight_key_values": true,
      "use_human_readable_labels": true,
      "emoji": {
        "fallback_level": "full"
      }
    }
  },
  "visualization": {
    "node_size_metric": "degree",
    "base_node_size": 6.0,
    "node_size_multiplier": 4.0,
    "scaling_algorithm": "logarithmic",
    "edge_thickness_metric": "weight",
    "base_edge_thickness": 1.0,
    "base_edge_width": 0.5,
    "edge_width_multiplier": 1.5,
    "edge_width_scaling": "logarithmic",
    "intra_community_color": "#c0c0c0",
    "bridge_color": "#6e6e6e",
    "show_labels": true,
    "show_tooltips": true,
    "show_legend": true,
    "font_size": 8,
    "node_alpha": 0.8,
    "edge_alpha": 0.3,
    "pyvis_interactive": {
      "width": "100%",
      "height": "90vh",
      "bgcolor": "#ffffff",
      "font_color": "#000000",
      "notebook": false,
      "physics_solver": "forceAtlas2Based"
    },
    "static_image": {
      "layout": "spring",
      "width": 1800,
      "height": 1800,
      "dpi": 150,
      "edge_arrow_size": 6
    },
    "layout": {
      "algorithm": "spring",
      "align_with_html": true,
      "spring": {
        "k": 0.2,
        "iterations": 30,
        "spring_length": 1.0,
        "spring_constant": 1.0,
        "repulsion_strength": 1.0,
        "attraction_strength": 1.0,
        "center_gravity": 0.01,
        "gravity_x": 0.0,
        "gravity_y": 0.0,
        "damping": 0.9,
        "min_velocity": 0.02,
        "max_displacement": 8.0,
        "enable_multistage": false,
        "initial_k_multiplier": 2.0,
        "final_k_multiplier": 0.7
      },
      "kamada_kawai": {
        "max_iterations": 1500,
        "tolerance": 1e-7,
        "distance_scale": 1.2,
        "spring_strength": 1.5,
        "pos_tolerance": 1e-5,
        "weight_function": "path"
      },
      "circular": {
        "radius": null,
        "center": null,
        "start_angle": 0.0,
        "angular_spacing": "uniform",
        "group_by_community": true,
        "community_separation": 0.3
      },
      "shell": {
        "shell_spacing": 1.5,
        "center_shell_radius": 0.3,
        "arrange_by_community": true,
        "arrange_by_centrality": false,
        "centrality_metric": "degree",
        "max_shells": 8,
        "nodes_per_shell": null
      }
    }
  }
}